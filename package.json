{
  "name": "react-ts-next-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "server": "node -r ts-node/register/transpile-only server.ts",
    "start": "yarn build && yarn server",
    "test": "jest --watch",
    "test-debug": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "lint:css": "stylelint './**/*styled.{js,ts}' --fix",
    "lint:css:in:js": "stylelint './**/*.js'",
    "lint:css:in:ts": "stylelint './**/*.tsx'",
    "ci:lint-js": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "ci:lint-css": "stylelint './**/*styled.{js,ts}'",
    "ci:jest": "jest --ci",
    "ci:pw-run": "yarn build && yarn pw-run",
    "print-env": "print-env",
    "heroku-prebuild": "yarn install --production",
    "pw-run": "npx playwright test",
    "pw-codegen": "npx playwright codegen playwright.dev",
    "pw-run-debug": "yarn playwright test --debug",
    "sb": "start-storybook -p 6006",
    "sb-build": "build-storybook",
    "types:check": "tsc --noEmit --pretty",
    "scan": "yarn lint && yarn types:check && yarn ci:jest",
    "scan:full": "yarn scan && yarn ci:pw-run",
    "analyze": "ANALYZE=true next build",
    "gen:types": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": "16.17.1"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@ebay/nice-modal-react": "^1.2.7",
    "@graphql-tools/jest-transform": "^1.2.1",
    "@next/bundle-analyzer": "^12.2.5",
    "ally.js": "^1.4.1",
    "apollo-link-token-refresh": "^0.4.0",
    "body-parser": "^1.19.0",
    "core-js": "3",
    "csstype": "^3.1.0",
    "date-fns": "^2.23.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.10.3",
    "http-proxy-middleware": "^2.0.6",
    "https-proxy-agent": "^5.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "next": "^13.1.6",
    "next-with-apollo": "^5.3.0",
    "polished": "^3.4.4",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.13.1",
    "react-select": "^5.4.0",
    "react-toastify": "^7.0.4",
    "storybook-addon-next-router": "^4.0.1",
    "storybook-formik": "^2.4.2",
    "styled-components": "^5.3.5",
    "ts-node": "^10.9.1",
    "universal-cookie": "^4.0.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@bahmutov/print-env": "^2.1.2",
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.2",
    "@playwright/test": "^1.25.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
    "@swc/core": "^1.3.32",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@swc/wasm": "^1.3.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "@types/react": "18.0.1",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-playwright": "^0.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.2.3",
    "jest": "^29.4.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^28.1.0",
    "jest-extended": "^3.1.0",
    "jest-fail-on-console": "^3.0.1",
    "jest-mock": "^29.5.0",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.0.3",
    "path-browserify": "^1.0.1",
    "postcss": "8",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.7.1",
    "react-select-event": "^5.5.1",
    "storybook-addon-apollo-client": "^4.0.13",
    "storybook-addon-swc": "^1.1.9",
    "stylelint": "^15.6.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.4",
    "webpack": "^5.75.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix", "stylelint --fix"
    ]
  }
}
